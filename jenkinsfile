#!groovy
properties([disableConcurrentBuilds()])

pipeline {
    agent {
        docker { image 'python:3.9.7' }
    }
    options {
        buildDiscarder(logRotator(numToKeepStr: '5', artifactNumToKeepStr: '5'))
        timestamps()
    }
    stages {
        stage("Create docker image") {
            steps {
                echo "======== building image ========"
                sh 'docker build . '
            }
        }
    }
}
